    "vue-i18n": "^8.12.0",
    "vue-moment": "^4.0.0",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
