{
  "name": "laravel-quasar",
  "version": "1.0.0",
  "description": "Laravel with Quasar Framework (SPA) by Manfred047.",
  "productName": "Laravel with Quasar Framework",
  "cordovaId": "com.manfred047",
  "keywords": [
    "laravel",
    "php",
    "vuejs",
    "vue",
    "quasar",
    "js",
    "phone",
    "tablet",
    "desktop",
    "spa",
    "pwa",
    "website",
    "electron"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manfred047/laravel-quasar.git"
  },
  "author": {
    "name": "Emanuel Chable Concepcion",
    "email": "manfred@manfred047.com",
    "url": "https://manfred047.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Manfred047/laravel-quasar/issues"
  },
  "homepage": "https://quasar-framework.org",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "q-ssr-build": "quasar build -m ssr",
    "q-ssr-dev": "quasar dev -m ssr",
    "q-spa-build": "quasar build",
    "q-spa-dev": "quasar dev -H lq.test"
  },
  "devDependencies": {
    "@quasar/app": "^1.8.10",
    "@quasar/quasar-app-extension-dotenv": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "webpack-require-http": "^0.4.3"
  },
  "dependencies": {
    "@quasar/extras": "^1.8.1",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "quasar": "^1.11.3",
    "vee-validate": "^3.3.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.7",
    "vue-moment": "^4.1.0",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.9.0"
  }
}
