{
    "name": "laravel-quasar",
    "version": "1.0.0",
    "description": "Laravel with Quasar Framework (SPA) by Manfred047.",
    "productName": "Laravel with Quasar Framework",
    "cordovaId": "com.manfred047",
    "keywords": [
        "laravel",
        "php",
        "vuejs",
        "vue",
        "quasar",
        "js",
        "phone",
        "tablet",
        "desktop",
        "spa",
        "pwa",
        "website",
        "electron"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Manfred047/laravel-quasar.git"
    },
    "author": {
        "name": "Emanuel Chable Concepcion",
        "email": "manfred@manfred047.com",
        "url": "https://manfred047.com/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Manfred047/laravel-quasar/issues"
    },
    "homepage": "https://quasar-framework.org",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "lint": "eslint --ext .js,.vue src",
        "q-ssr-build": "quasar build -m ssr",
        "q-ssr-dev": "quasar dev -m ssr",
        "q-spa-build": "quasar build",
        "q-spa-dev": "quasar dev -H lq.test"
    },
    "devDependencies": {
        "@quasar/app": "^1.4.3",
        "@quasar/quasar-app-extension-dotenv": "^1.0.0",
        "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.3",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-vue": "^6.0.2",
        "laravel-mix": "^5.0.1",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.24.0",
        "sass-loader": "^8.0.0",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "webpack-require-http": "^0.4.3"
    },
    "dependencies": {
        "@quasar/extras": "^1.3.3",
        "axios": "^0.19.0",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.27",
        "quasar": "^1.5.11",
        "vee-validate": "^3.2.1",
        "vue": "^2.6.11",
        "vue-i18n": "^8.15.3",
        "vue-moment": "^4.1.0",
        "vue-recaptcha": "^1.3.0",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.2"
    },
    "engines": {
        "node": ">= 8.9.0",
        "npm": ">= 5.6.0",
        "yarn": ">= 1.6.0"
    },
    "browserslist": [
        "last 1 version, not dead, ie >= 11"
    ]
}
