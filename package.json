  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Manfred047/laravel-quasar/issues"
  },
  "homepage": "https://quasar-framework.org",
  "private": true,
  "scripts": {
    "mix-dev": "npm run development",
    "mix-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix-watch": "npm run development -- --watch",
    "mix-watch-poll": "npm run watch -- --watch-poll",
    "mix-hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix-prod": "npm run production",
    "mix-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint --ext .js,.vue src",
    "q-ssr-build": "quasar build -m ssr",
    "q-ssr-dev": "quasar dev -m ssr",
    "q-spa-build": "quasar build",
    "q-spa-dev": "quasar dev"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.4",
    "@quasar/quasar-app-extension-dotenv": "^1.0.0-beta.10",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "laravel-mix": "^4.1.2",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "symlink-webpack-plugin": "^0.2.0",
    "webpack-require-http": "^0.4.3"
  },
  "dependencies": {
    "@quasar/extras": "^1.2.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "popper.js": "^1.15.0",
